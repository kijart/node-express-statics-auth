<!doctype html>
<html>
	<head>
		<meta charset="utf-8">
		<title>Static files with authentication</title>
	</head>
	<body>
		{{#if user}}
			<p>
				<a href="/">Home</a> |
				<a href="/docs/">Docs</a> |
				<a href="/auth/logout">Log Out</a>
			</p>
		{{else}}
			<p>
			<a href="/">Home</a> |
			<a href="/auth/google">Log In</a>
			</p>
		{{/if}}
		{{{ body }}}
	</body>
</html>
